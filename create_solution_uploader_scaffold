#rake db:migrate
#rails g controller DSKBSolutions index new create destroy
rails g model Indexitem name:string attachment:string title:string description:string record_type:string category:string format:string url:string authors:string publication_date:string tools_used:string concepts_used:string data_sources:string attachment_url:string
